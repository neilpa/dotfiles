[diff]
	tool = odd
[difftool]
	prompt = false
[difftool "odd"]
	cmd = "odd.exe $LOCAL $REMOTE"
[alias]
	ci = commit
	st = status
	wd = difftool --dir-diff
[color]
	diff = auto
	status = auto
[color "diff"]
	meta = white
	frag = cyan bold
	old = red bold
	new = green bold
[color "status"]
	added = green bold
	changed = red
	untracked = cyan bold
[core]
	excludesfile = ~/.gitignore
